{
	"name": "12ft",
	"description": "Remove Paywalls using 12ft ladder",
	"version": "0.2",
	"author": "Yankee Maharjan",
	"manifest_version": 2,
	"background": {
		"scripts": [
			"main.js"
		],
		"persistent": false
	},
	"permissions": [
		"activeTab"
	],
	"browser_action": {
		"default_icon": {
			"16": "assets/16x16.png",
			"48": "assets/48x48.png",
			"128": "assets/128x128.png"
		}
	}
}
